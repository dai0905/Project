@model Project.Data.ThuongHieu

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Edit</h1>

<h4>Thương Hiệu</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="ThuongHieus" asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="text" asp-for="MaThuongHieu" readonly />

            <div class="form-group">
                <label asp-for="TenThuongHieu" class="control-label"></label>
                <input asp-for="TenThuongHieu" class="form-control" />
                <span asp-validation-for="TenThuongHieu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-controller="ThuongHieus" asp-action="Index">Back to List</a>
</div>

